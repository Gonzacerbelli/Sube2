<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.RamalEstacion" table="ramal_tiene_estacion">
		<id column="idRelacion" name="idRelacion">
			<generator class="identity" />
		</id> 
		<property column="nroEstacion" name="nroEstacion" type="int" />
		<many-to-one name="estacion" class="datos.Estacion" fetch="select">
            <column name="idEstacion" />
        </many-to-one>
        <many-to-one name="ramal" class="datos.Ramal" fetch="select">
            <column name="idRamal" />
        </many-to-one>
	</class>
</hibernate-mapping>