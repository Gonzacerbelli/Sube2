<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="datos.Boleto" table="boleto">
		<id column="idBoleto" name="idBoleto">
			<generator class="identity" />
		</id>
		
		<property column="fechaHora" name="fechaHora" type="calendar" />
		
		<property column="precioFinal" name="precioFinal" type="double" />
		
		<many-to-one name="tarifa" class="datos.Tarifa" column="idTarifa" />
		
		<many-to-one name="estacion" class="datos.Estacion" column="idEstacion" not-null="true"/>
		
		<many-to-one name="ramal" class="datos.Ramal" column="idRamal" not-null="true"/>
		
		<many-to-one name="linea" class="datos.Linea" column="idLinea" />
		
		<many-to-one name="transporte" class="datos.Transporte" column="idTransporte"/>
		
		<many-to-one name="viaje" class="datos.Viaje" column="idViaje"/>
		
	</class>
</hibernate-mapping>